<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <font app:font="@font/inter_light"
        app:fontStyle="normal"
        app:fontWeight="300"
        android:font="@font/inter_light"
        android:fontStyle="normal"
        android:fontWeight="300" />

    <font app:font="@font/inter_regular"
        app:fontStyle="normal"
        app:fontWeight="400"
        android:font="@font/inter_regular"
        android:fontStyle="normal"
        android:fontWeight="400"/>

    <font app:font="@font/inter_semibold"
        app:fontStyle="normal"
        app:fontWeight="600"
        android:font="@font/inter_semibold"
        android:fontStyle="normal"
        android:fontWeight="600"/>

    <font app:font="@font/inter_bold"
        app:fontStyle="normal"
        app:fontWeight="700"
        android:font="@font/inter_bold"
        android:fontStyle="normal"
        android:fontWeight="700"/>
</font-family>